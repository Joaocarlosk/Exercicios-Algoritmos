algoritmo "semnome"
var
   n1,n2: inteiro
inicio
   Escreva ("N1: ")
   leia (n1)
   Escreva ("N2: ")
   leia (n2)
   Se n1 > n2 entao
      Escreva ("O primeiro valor é maior.")
      senao
         Se n2 > n1 entao
            Escreva ("O segundo valor é maior.")
         senao
            Escreva ("Não existe valor maior os dois são iguais.")
         Fimse
   Fimse
fimalgoritmo